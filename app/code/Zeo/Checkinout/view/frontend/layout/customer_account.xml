<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
    <referenceBlock name="customer_account_navigation">
      			<block class="Zeo\Checkinout\Block\EmployeeLink" name="zeo_checkinout_customer-account-navigation-account-employeelink">
                    <arguments>
                        <argument name="label" xsi:type="string" translate="true">Check in/out</argument>
                        <argument name="path" xsi:type="string">checkinout/index</argument>
                    </arguments>
                </block>
                <block class="Zeo\Checkinout\Block\ManagerLink" name="zeo_checkinout_customer-account-navigation-account-managerlink">
                    <arguments>
                        <argument name="label" xsi:type="string" translate="true">Employees</argument>
                        <argument name="path" xsi:type="string">checkinout/employee</argument>
                    </arguments>
                </block>
                 <block class="Zeo\Checkinout\Block\ManagerLink" name="zeo_checkinout_customer-account-navigation-account-managerreportlink">
                    <arguments>
                        <argument name="label" xsi:type="string" translate="true">Report</argument>
                        <argument name="path" xsi:type="string">checkinout/report</argument>
                    </arguments>
                </block>
    </referenceBlock>
    
    </body>
</page>